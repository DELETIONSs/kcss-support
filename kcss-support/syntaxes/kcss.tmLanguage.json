{
	"fileTypes": ["kcss"],
	"name": "KCSS",
	"scopeName": "source.kcss",
	"patterns": [
	  {
		"name": "keyword.control.kcss",
		"match": "(>mixin|>function|>var)"
	  },
	  {
		"name": "variable.other.kcss",
		"match": "\\$[a-zA-Z_][a-zA-Z0-9_]*"
	  },
	  {
		"name": "keyword.operator.kcss",
		"match": "(:|;|,)"
	  },
	  {
		"name": "comment.block.kcss",
		"begin": "/\\*",
		"end": "\\*/"
	  },
	  {
		"name": "comment.line.double-slash.kcss",
		"match": "//.*$"
	  },
	  {
		"name": "string.quoted.double.kcss",
		"begin": "\"",
		"end": "\"",
		"patterns": [
		  {
			"name": "constant.character.escape.kcss",
			"match": "\\\\."
		  }
		]
	  }
	]
  }
  